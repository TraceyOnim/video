<h1> Login User </h1>
<%= form_for @conn, Routes.session_path(@conn, :create), [as: :session], fn f -> %>
<div>
	<%= label f, :email, "Email" %>
	<%= text_input f, :email%>
</div>

<div>
	<%= label f, :password, "Password" %>
	<%= password_input f, :password%>
</div>

<%= submit "Log in"%>
<% end %>