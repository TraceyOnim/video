<h1> List of users </h1>
<table>
	<tr>
		<th>id</th>
		<th>Name</th>
		<th>Username</th>	
		<th></th>
	</tr>
	<%= for user <- @users do %>
	<tr>
		<td> <%= user.id%> </td>
		<td> <%= user.name%> </td>
		<td> <%= user.username%> </td>
		<td><%= link "View", to: Routes.user_path(@conn, :show, user.id), class: "btn" %></td>
	</tr>
	<% end %>	
</table>




